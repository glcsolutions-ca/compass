{
  "name": "@compass/sdk",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "generate": "openapi-typescript ../contracts/openapi/openapi.json --output src/generated/schema.ts",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc -p tsconfig.ref.json --noEmit"
  },
  "dependencies": {
    "@compass/contracts": "workspace:*",
    "openapi-fetch": "^0.17.0"
  },
  "devDependencies": {
    "openapi-typescript": "7.13.0"
  }
}
