{
  "name": "@compass/testkit",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc -p tsconfig.ref.json --noEmit"
  }
}
